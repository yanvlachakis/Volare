# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
PRIVATE_KEY=your_private_key_here

# Trading Parameters
MIN_LIQUIDITY=1000
MIN_VOLUME=5000
MAX_SLIPPAGE=0.01
START_BALANCE=50
TRADE_SIZE_PERCENT=0.05
MIN_PROFIT_THRESHOLD=0.002

# Risk Management Parameters
MAX_POSITION_SIZE=0.2
MAX_DRAWDOWN=0.1
MAX_DAILY_LOSS=0.05
MAX_LEVERAGE=1.5
MIN_PROFIT_TARGET=0.005
TRAILING_STOP_DISTANCE=0.01
STOP_LOSS_DISTANCE=0.02
RISK_PER_TRADE=0.02
MAX_CORRELATION=0.7
VOLATILITY_THRESHOLD=0.4

# Profit Management Parameters
PROFIT_THRESHOLD=0.005
REINVESTMENT_RATIO=0.7
PROFIT_TAKING_INTERVALS=0.01,0.02,0.05
MAX_REINVESTMENT=1000
RESERVE_RATIO=0.2
COMPOUND_FREQUENCY=3
MAX_DRAWDOWN_RESET=0.05
PROFIT_LOCK_PERIOD=7

# Resource Management
MAX_MONTHLY_BUDGET=50
MIN_RPC_NODES=1
MAX_RPC_NODES=3
RPC_COST_PER_NODE=15
MIN_PORTFOLIO_SIZE=50
SCALING_THRESHOLD=0.2
CPU_THRESHOLD=0.8
MEMORY_THRESHOLD=0.8

# Quantitative Strategy Parameters
MIN_SPREAD=0.001
MAX_POSITION_HOLD_TIME=300
MIN_VOLUME_THRESHOLD=100
CORRELATION_THRESHOLD=0.7
MEAN_REVERSION_THRESHOLD=2.0
MARKET_MAKING_DEPTH=3
HFT_TIMEOUT=0.5

# AI Model Parameters
LOOKBACK_PERIOD=30
CONFIDENCE_THRESHOLD=0.7
MODEL_UPDATE_INTERVAL=1800

# Monitoring
LOG_LEVEL=INFO
ENABLE_TELEGRAM_NOTIFICATIONS=false
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Performance Tracking
ENABLE_PERFORMANCE_TRACKING=true
TRACKING_INTERVAL=300
METRICS_EXPORT_PATH=./metrics
ENABLE_TRADE_LOGGING=true

# Risk Alerts
ENABLE_RISK_ALERTS=true
ALERT_THRESHOLD_DRAWDOWN=0.05
ALERT_THRESHOLD_EXPOSURE=0.8
ALERT_THRESHOLD_VOLATILITY=1.5

# Position Management
MAX_POSITIONS=3
MAX_POSITIONS_PER_ASSET=1
MIN_POSITION_DURATION=60
MAX_POSITION_DURATION=3600
ENABLE_POSITION_NETTING=true

# Infrastructure Scaling
ENABLE_AUTO_SCALING=true
MIN_SCALING_INTERVAL=86400
PERFORMANCE_HISTORY_DAYS=30
MAX_CPU_UTILIZATION=80
MAX_MEMORY_UTILIZATION=80
BACKUP_RPC_URLS=["https://api.mainnet-beta.solana.com","https://solana-api.projectserum.com"] 